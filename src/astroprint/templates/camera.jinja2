<script type="text/template" id="camera-watch-page-template">
	<div class="row large-collapse">
		<div class="small-12 columns title-heading">
			<h1 class="bold">Camera</h1>
		</div>
	
		<div class="camera-view">
			<div class="row video-container">
				<div class="small-12 columns">
					<div class="video-off">
						<i class="icon-camera"></i>
						<h3 class="bold">Camera Stream Off</h3>
					</div>
					<!-- <div class="no-detection">
						<i class="icon-attention"></i>
						<h3 class="bold">No Camera Detected</h3>
					</div> -->
				</div>
				<video id="remotevideo" class="cover-img-cont" autoplay="" style="display:none">    
				    <source src="http://localhost:8004" type="video/ogg">
				       You browser doesn't support element <code>video</code>.
				</video>
			</div>
			<div class="row buttons">
				<div class="small-12 large-4 large-centered columns">
					<button class="success radius bold"><i class="icon-play"></i>&nbsp;&nbsp;Start Streaming Video</button>
				</div>
				<div class="small-12 large-4 large-centered columns">
					<button class="secondary radius bold"><i class="icon-stop"></i>&nbsp;&nbsp;Stop Streaming Video</button>
				</div>
			</div>
		</div>
	</div>
	
	<script type="text/javascript">
		selectedStream = ('<%=settings.encoding%>' == 'h264' ? 1 : 2 );
		sizeVideo = '<%=settings.size%>';
		janus = null;
	</script>
</script>
